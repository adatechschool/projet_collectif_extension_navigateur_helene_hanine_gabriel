{
    "manifest_version" : 3,
    "name" : "Diario",
    "version" : "1.0.1",
    "description" : "This extension will help you to personalize your new Google Chrome tabs",
    "permissions" : [
        "tabs",
        "geolocation",
        "bookmarks"
    ],
    "action" : {
        "default_icon" : "img/season.png",
        "default_title" : "Diario"
    },
    "icons" : {
        "128" : "img/season.png"
    },
    "chrome_url_overrides" : {
        "newtab" : "index.html"
    }
}